{
  "name": "alle/assets-merger",
  "type": "kohana-module",
  "description": "Managing and merging assets for Kohana",
  "homepage": "https://github.com/alle/assets-merger",
  "license": "BSD-3-Clause",
  "keywords": ["kohana", "assets"],
  "authors": [
    {
      "name": "OpenBuildings",
      "homepage": "http://www.openbuildings.com/",
      "role": "developer"
    },
    {
      "name": "Florian Rodriguez",
      "homepage": "http://www.negative-network.com",
      "role": "developer"
    },
    {
      "name": "Alessandro Frangioni",
      "email": "alessandro.frangioni@gmail.com",
      "homepage": "https://github.com/alle",
      "role": "developer"
    }
  ],
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "cssmin/cssmin",
        "version": "3.0.1",
        "dist": {
          "url": "http://cssmin.googlecode.com/files/cssmin-v3.0.1.php",
          "type": "file"
        },
        "autoload": {
          "classmap": ["cssmin-v3.0.1.php"]
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "mrclay/minify",
        "version": "2.1.7",
        "dist": {
          "url": "http://minify.googlecode.com/files/minify-2.1.7.zip",
          "type": "zip"
        },
        "autoload": {
          "classmap": ["min/lib/"]
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "mrclay/minify",
        "version": "2.1.7",
        "dist": {
          "url": "http://minify.googlecode.com/files/minify-2.1.7.zip",
          "type": "zip"
        },
        "autoload": {
          "classmap": ["min/lib/"]
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "yui/yuicompressor",
        "type": "component",
        "version": "2.4.8",
        "dist": {
          "url": "https://github.com/yui/yuicompressor/releases/download/v2.4.8/yuicompressor-2.4.8.jar",
          "type": "file"
        },
        "autoload": {
          "classmap": ["yuicompressor-2.4.8.jar"]
        }
      }
    }
  ],
  "require": {
    "composer/installers": "~1.0",
    "kohana/core": ">=3.3",
    "php": ">=5.3.3",
    "natxet/CssMin": "~3.0",
    "mrclay/minify": ">=2.1.7",
    "leafo/lessphp": "0.4.0",
    "coffeescript/coffeescript": ">=1.3",
    "richthegeek/phpsass": ">=2014-03-20",
    "yui/yuicompressor": "2.4.8"
  },
  "minimum-stability": "dev"
}
